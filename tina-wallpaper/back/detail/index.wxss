.page {
    left: 0;
    position: absolute;
    top: 0;
    width: 100%;
}

.index,.page {
    background-color: #000;
    height: 100%;
}

.index {
    min-height: 100%;
    overflow: scroll;
    position: relative;
    width: 750rpx;
}

.topCon {
    left: 0;
    position: fixed;
    top: 0;
    width: 100%;
}

.indexImg {
    width: 750rpx;
}

.myVideo {
    height: 100%;
    width: 100%;
}

.backIco {
    left: 16rpx;
    position: absolute;
    top: 50%;
    transform: rotateY(180deg) translateY(50%);
    width: 16rpx;
}

.indexTit {
    color: #fff;
    font-size: 32rpx;
    font-weight: 700;
    height: 100%;
    width: 100%;
}

.barList {
    background: hsla(0,0%,100%,.2);
    border: 2rpx solid #f5f5f5;
    border-radius: 32rpx;
    bottom: 300rpx;
    padding: 30rpx 0;
    position: fixed;
    right: 30rpx;
    width: 126rpx;
}

.barListItem {
    margin-top: 50rpx;
}

.barItem {
    height: 60rpx;
    width: 60rpx;
}

.barItemTit {
    color: #fff;
    font-size: 28rpx;
    line-height: 30rpx;
    margin-top: 10rpx;
    text-align: center;
    text-shadow: #000 .1em .1em .2em;
    width: 100%;
}

.userInfo {
    width: 100%;
}

.userImg {
    background: #fff;
    border: 4rpx solid #fff;
    border-radius: 50%;
    height: 92rpx;
    width: 92rpx;
}

.userName {
    color: #fff;
    font-size: 26rpx;
    margin-top: 12rpx;
    text-align: center;
    text-shadow: #000 .1em .1em .2em;
    width: 100%;
}

.saveDesc {
    color: red;
    font-size: 24rpx;
    margin-top: 20rpx;
}

.saveCon {
    background-color: rgba(0,0,0,.8);
    height: 100%;
    left: 0;
    position: fixed;
    top: 0;
    width: 100%;
    z-index: 100;
}

.saveInfo {
    background: #fff;
    border-radius: 20rpx;
    height: max-content;
    padding: 80rpx 30rpx 60rpx;
    position: relative;
    width: 650rpx;
}

.saveCloseIco {
    height: 42rpx;
    position: absolute;
    right: 30rpx;
    top: -66rpx;
    width: 42rpx;
}

.saveItem {
    width: 100%;
}

.saveItemInfo {
    flex-shrink: 0;
    width: 350rpx;
}

.saveTit {
    color: #000;
    font-size: 34rpx;
    font-weight: 700;
    margin-bottom: 30rpx;
}

.saveTxt {
    color: #afafb3;
    font-size: 26rpx;
}

.saveBtn {
    background: #2d2100;
    border-radius: 20rpx;
    color: #fadd9b;
    font-size: 28rpx;
    height: 76rpx;
    width: 220rpx;
}

.saveBtnIco {
    height: 28rpx;
    margin-right: 10rpx;
    width: 30rpx;
}

.saveItem1 {
    margin-top: 40rpx;
}

.saveItem1 .saveBtn {
    background: #c8c8cc;
}

.saveItem1 .saveBtnIco {
    height: 26rpx;
    margin-right: 10rpx;
    width: 26rpx;
}

.shareBtn {
    background-color: initial;
    flex-shrink: 0;
    margin: 0;
    padding: 0;
    width: 60rpx;
}

.shareBtn::after {
    background-color: none;
    background: transparent;
    border: 1px solid transparent;
    border-radius: 0;
    content: "";
    display: none;
    height: 100%;
    padding: 0;
    transform: scale(0);
    width: 100%;
}

button,button::after {
    line-height: 0;
}