.jiangqie-loadmore {
    font-size: 24rpx;
    line-height: 1.5em;
    margin: 1.5em auto;
    text-align: center;
    width: 48%;
}

.jiangqie-loading-1 {
    animation: a 1s steps(12) infinite;
    background: transparent url(data:image/svg+xml;base64,) no-repeat;
    background-size: 100%;
    display: inline-block;
    height: 20px;
    margin: 0 5px;
    vertical-align: middle;
    width: 20px;
}

@-webkit-keyframes a {
    0% {
        transform: rotate(0deg);
    }

    to {
        transform: rotate(1turn);
    }
}

@keyframes a {
    0% {
        transform: rotate(0deg);
    }

    to {
        transform: rotate(1turn);
    }
}

.jiangqie-loading-2,.jiangqie-loadmore-tips {
    display: inline-block;
    vertical-align: middle;
}

.jiangqie-loading-2 {
    animation: rotate 1s linear infinite;
    border: 1px solid #8f8d8e;
    border-radius: 50%;
    clip-path: polygon(0 0,100% 0,100% 30%,0 30%);
    height: 28rpx;
    margin: 0 6px;
    width: 28rpx;
}

@-webkit-keyframes rotate {
    from {
        transform: rotate(0deg);
    }

    to {
        transform: rotate(1turn);
    }
}

@keyframes rotate {
    from {
        transform: rotate(0deg);
    }

    to {
        transform: rotate(1turn);
    }
}

.jiangqie-loading-3 {
    animation: jiangqie-rotate .7s linear infinite;
    background: 0 0;
    border: 2px solid #e5e5e5;
    border-left-color: #8f8d8e;
    border-radius: 50%;
    display: inline-block;
    height: 28rpx;
    margin: 0 6px;
    vertical-align: middle;
    width: 28rpx;
}

.jiangqie-loading-3.jiangqie-loading-primary {
    border-color: #e5e5e5 #e5e5e5 #e5e5e5 #5677fc;
}

.jiangqie-loading-3.jiangqie-loading-green {
    border-color: #e5e5e5 #e5e5e5 #e5e5e5 #19be6b;
}

.jiangqie-loading-3.jiangqie-loading-orange {
    border-color: #e5e5e5 #e5e5e5 #e5e5e5 #ff7900;
}

.jiangqie-loading-3.jiangqie-loading-red {
    border-color: #ededed #ededed #ededed #ed3f14;
}

@-webkit-keyframes jiangqie-rotate {
    0% {
        transform: rotate(0);
    }

    100% {
        transform: rotate(1turn);
    }
}

@keyframes jiangqie-rotate {
    0% {
        transform: rotate(0);
    }

    100% {
        transform: rotate(1turn);
    }
}